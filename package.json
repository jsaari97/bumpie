{
  "name": "bumpie",
  "description": "Bump asset versions on package.json increment",
  "version": "0.0.0",
  "author": "Jim Saari @jsaari97",
  "bin": {
    "bumpie": "./bin/run"
  },
  "bugs": "https://github.com/jsaari97/bumpie/issues",
  "dependencies": {
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/node": "^12.0.0",
    "@types/yargs": "^15.0.5",
    "ava": "^3.11.1",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/jsaari97/bumpie",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "bumpie"
  },
  "repository": "jsaari97/bumpie",
  "scripts": {
    "pretest": "rm -rf lib && tsc -b",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "ava",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "lib/"
      }
    }
  }
}
